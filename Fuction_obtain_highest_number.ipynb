        "#Function that obtains the highest number from a list \n",
        "\n",
        "def obtain_max_number(C):\n",
        "  max_value = 0 \n",
        "  for i in C:\n",
        "    if i > max_value:\n",
        "      max_value = i\n",
        "  print(f\"The highest number is {max_value}\")\n",
        "obtain_max_number(C = [180, 90 , 5, 68, 23, 553 , 1080, 8])\n",
        "\n"
      ]
    }
